<%@ val results: String %>
<html>
<head><title>Disqus comment search</title></head>
<body>
    <form action="/search">
    Disqus username: <input type="text" name="username"/>
    Search terms: <input type="text" name="query"/>
    <input type="submit" value="Submit"/>
    </form>
    <%= unescape(results) %>
</body>
</html>